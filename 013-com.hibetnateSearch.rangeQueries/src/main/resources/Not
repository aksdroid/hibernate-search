												.onField("dateOfBirth")
												.above(lastDate)
												.excludeLimit()
												.createQuery();
				
		org.apache.lucene.search.Query query = qb.range()
												.onField("dateOfBirth")
												.before(firstDate)
												.excludeLimit()
												.createQuery();
		
